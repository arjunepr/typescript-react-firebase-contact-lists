{
  "name": "react-contact-lists",
  "version": "1.0.0",
  "description": "An Electron Contact Lists App that makes use of React on the front end, and SQLite as a database.",
  "main": "main.js",
  "author": "Arjun Raju Pillai <revarjunr@gmail.com>",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch webpack.config.js --exec 'rm -rf dist && webpack-dev-server'",
    "build": "NODE_ENV=production rm -rf dist && webpack -p",
    "firebase-deploy": "yarn run build && firebase init && firebase deploy"
  },
  "dependencies": {
    "@types/nedb": "^1.8.3",
    "@types/node": "^8.0.10",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "@types/react-router-dom": "^4.0.6",
    "nedb": "^1.8.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "reset-css": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "css-loader": "^0.28.4",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.11.2",
    "firebase-tools": "^3.9.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "nodemon": "^1.11.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
